{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN shoping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "client": "npm start --prefix frontend",
    "server": "nodemon backend/server",
    "dev": " concurrently \"npm run server\" \"npm run client\" ",
